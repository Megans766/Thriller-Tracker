<%- include('../partials/html-head') %>
<%- include('../partials/nav') %>

<main>
  <div>
    <% profiles.forEach(profile => { %>
      <a href="/profiles/<%= profile._id %>">
        <div>
          <img src="<%= profile.avatar %>" alt="<%= profile.name %>'s avatar">
          <div>
            <h2><%= profile.name %></h2>
          </div>
        </div>
      </a>
    <% }) %>
  </div>
</main>

<%- include('../partials/footer') %>